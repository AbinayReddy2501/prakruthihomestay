{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://api.prakruthi.srinovatech.site/api",
    "REACT_APP_RAZORPAY_KEY_ID": "rzp_test_Nb4Z9WyKMSAjzu"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://prakruthi.srinovatech.site"
        }
      ]
    }
  ],
  "domains": [
    "prakruthi.vercel.app",
    "prakruthi.srinovatech.site"
  ]
}
